{
  "manifest_version": 2,
  "name": "Gitako Enterprise - Github File Tree",
  "version": "0.8.8",
  "description": "The missing part of GitHub. (enhanced)",
  "author": "EnixCoda & TheoSun",
  "icons": {
    "128": "icons/Gitako-128x128.png"
  },
  "homepage_url": "https://github.com/Soontao/Gitako",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "firefox-shim.js",
        "browser-polyfill.js",
        "content.js"
      ]
    }
  ]
}